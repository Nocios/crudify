{
  "name": "@nocios/crudify",
  "version": "1.0.2",
  "description": "Lightweight JS SDK for Crudify GraphQL API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "dotenv -e .env.dev -- ts-node test.ts",
    "start:stg": "dotenv -e .env.stg -- ts-node test.ts",
    "prepublishOnly": "dotenv -e .env.prod -- npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nocios/crudify.git"
  },
  "bugs": {
    "url": "https://github.com/nocios/crudify/issues"
  },
  "homepage": "https://github.com/nocios/crudify#readme",
  "keywords": [
    "nocios",
    "crudify",
    "crud",
    "api",
    "typescript",
    "sdk"
  ],
  "author": "Nocios",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "undici-types": "^7.5.0"
  }
}

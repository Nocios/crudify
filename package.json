{
  "name": "@nocios/crudify-browser",
  "version": "4.0.0",
  "description": "Browser-specific Lightweight JS SDK for Crudify GraphQL API with Refresh Token Pattern support",
  "author": "Nocios",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "start": "vite example",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nocios/crudify.git"
  },
  "bugs": {
    "url": "https://github.com/nocios/crudify/issues"
  },
  "homepage": "https://github.com/nocios/crudify#readme",
  "keywords": [
    "nocios",
    "crudify",
    "crud",
    "api",
    "typescript",
    "sdk",
    "browser",
    "refresh-token",
    "jwt",
    "authentication",
    "security"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.3.5"
  }
}
